{
  "name": "zentty-client",
  "version": "0.0.1",
  "description": "Official Zentty JavaScript Client",
  "main": "build/Release/index.js",
  "scripts": {
    "build": "babel src --presets babel-preset-es2015,stage-2 --out-dir dist",
    "browser-build": "browserify src/index.js -o browser-dist/zentty-client.js -t [ babelify --presets [ es2015 stage-2 ] ]",
    "test": "mocha --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medatech/zentty-client.git"
  },
  "author": "Martin Adams",
  "license": "MIT",
  "homepage": "https://github.com/medatech/zentty-client",
  "dependencies": {
    "apollo-client": "^1.9.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "base-64": "^0.1.0",
    "graphql-tag": "^2.4.2",
    "isomorphic-form-data": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "isomorphic-unfetch": "^2.0.0",
    "mocha": "^3.5.0"
  }
}
